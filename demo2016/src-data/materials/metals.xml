<?xml version="1.0" ?>

<material
	name = "stripetile"
	material_p3d = "stripetile"
	>
  <shader file = "data/shaders/cook_torrance.xml" preshader = "data/shaders/preshader.xml">
    <parameters>
      <diffuse_map value = "data/images/2014/stripetile.jpg"/>
      <specular_map value = "data/images/2014/stripetile.jpg"/>
      <shininess_map value = "data/images/2014/brightmetal_spc.jpg"/>
      <!-- ptkotkaspc.jpg too white -->
      <normal_map value = "data/images/2014/brightmetal_n.jpg"/>
      <!-- <occlusion_map value = "data/images/2014/ptkotkaocc.jpg"/ -->
      <occlusion_map shared_value = "SSAOBuffer"/>
      <reflection_map value = "data/images/title/gldc.jpg"/>
      <ambient_color value = "0.0 0.0 0.0"/>
      <diffuse_color value = "0.3 0.3 0.3"/>
      <specular_color value = "0.5 0.5 0.5"/>
      <normal_scale value = "1.00"/>
      <fresnel_normal_incidence value = "0.45"/>
      <roughness value = "0.80"/>
      <!-- 0.30 -->
      <diffuse_roughness value = "0.8"/>
      <diffuse_specular_shininess_minmax value = "0 0.8 0 0.8"/>
      <reflection_coeff value = "0.25"/>
    </parameters>
  </shader>
</material>

