<?xml version="1.0" ?>

<sequences>
	<scene_sequence
	    name = "loppu_part2"
		scene = "data/scenes/loppu_part2.p3d_scene" 
		camera = "Camera01"		
		frames_per_second = 30 
		start_time = "0.0"
		start_frame = "0"
		end_frame = "2000" 
        near_plane = "1"
        far_plane = "10000"
        ssao_enabled = "true">					
		</scene_sequence>

	
	<!-- Pitää olla jotta noise toimii, ei ole päällä mutta vaaditaan tämä rivi -->
	<image name = "noise_tex" path = "data/images/noise.jpg" />
	
		<!-- Post processing päällä -->	
	<!-- Tähän ei tarvitse koskea -->
	<post_processing name = "Post processing">
		<clear_color v = "0 0 0"/>
		<input sequence = "loppu_part2" time="0"/>			
		<!-- input sequence ="draw_alkuspace"/ -->	

	</post_processing>
	
	<!-- Depth buffer päällä -->
	<!-- Tähän ei tarvitse koskea -->
	<depth_buffer name = "Depth test">
        <clear_color v = "0 0 0"/>
		<input sequence = "Post processing"/>
	</depth_buffer>

	<!--  Linkataan Depth test sequence -->
	<!--  Ei tarttis koskee -->
	<composition name = "main timeline">	
		<input sequence = "Depth test"/>
	</composition>

	<!-- Main timeline -->
	<!--  Ei tarttis koskee -->		
	<timeline>
		<input sequence = "main timeline"/>
	</timeline>
</sequences>