<?xml version="1.0" ?>

<material
	name = "pomppuvaloboxi"
	material_p3d = "pomppuvaloboxi"
	>
    <shader file = "data/shaders/cook_torrance.xml" preshader = "data/shaders/preshader.xml">
        <parameters>
            <diffuse_color value = "0.5 0.5 0.5"/>
            <specular_color value = "0.5 0.5 0.5"/>
            <fresnel_normal_incidence value = "0.5"/>
            <roughness value = "0.5"/>
        </parameters>
	</shader>    
</material>

